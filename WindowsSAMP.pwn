	for(new x; x<21; x++)PlayerTextDrawHide(playerid, DesktopMusic[x][playerid]);
	for(new x; x<7; x++)PlayerTextDrawHide(playerid, Command[x][playerid]);
	return 1;
}

stock HideAllTextDraws(playerid)
{
  	if(IsPlayerConnected(playerid))
  	{
    	for(new i; i < 2048; i++)
    	{
      		if(PlayerText:i == PlayerText:INVALID_TEXT_DRAW || Text:i == Text:INVALID_TEXT_DRAW) continue;
      		PlayerTextDrawHide(playerid, PlayerText:i);
      		TextDrawHideForPlayer(playerid, Text:i);

    	}
  	}
	return 1;
}

stock SetDesktopImage(playerid, textname[])
{
	PlayerTextDrawSetString(playerid, Desktop[0][playerid], textname);
	PlayerTextDrawSetString(playerid, Start[15][playerid], textname);
	GetPlayerName(playerid, pname, sizeof(pname));
	format(string1, 29, "%s.txt", pname);
	dini_Set(string1, "Desktop", textname);
	return 1;
}

stock SetMetroColor(playerid, color)
{
	PlayerTextDrawBoxColor(playerid, Start[0][playerid], color);
	PlayerTextDrawColor(playerid, DownPanel[playerid], color);
	GetPlayerName(playerid, pname, sizeof(pname));
	format(string1, 29, "%s.txt", pname);
	dini_IntSet(string1, "MenuColor", color);
	return 1;
}
stock SetImage(playerid, image[]) return TextDrawSetString(Images[1][playerid], image);