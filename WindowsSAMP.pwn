public OnPlayerCommandText(playerid, cmdtext[])
{
	if (strcmp("/winstart", cmdtext, true, 10) == 0)
	{
		ShowStart(playerid);
		SelectTextDraw(playerid, 0xFF000022);
		return 1;
	}
	if (strcmp("/winclose", cmdtext, true, 10) == 0)
	{
		HideAllTextDraws(playerid);
	    CancelSelectTextDraw(playerid);
	    SendClientMessage(playerid, 0xFF0000FF, "To return in Windows RED, press /winstart");
		return 1;
	}
	return 0;
}

