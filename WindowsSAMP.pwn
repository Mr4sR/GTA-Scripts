public OnPlayerUpdate(playerid)
{
	new hour, minut, second, day, month, year, time[10], date[20];
	gettime(hour, minut, second);
	getdate(day, month, year);
	format(time, 10, "%02d:%02d", hour, minut);
	format(date, 20, "%02d.%02d.%02d", day, month, year);
    PlayerTextDrawSetString(playerid, Start[29][playerid], time);
    PlayerTextDrawSetString(playerid, Desktop[10][playerid], time);
    PlayerTextDrawSetString(playerid, Right[10][playerid], time);
    PlayerTextDrawSetString(playerid, Right[11][playerid], date);
    TextDrawSetString(Time[2][playerid], time);
    TextDrawSetString(Time[3][playerid], date);
    return 1;
}

